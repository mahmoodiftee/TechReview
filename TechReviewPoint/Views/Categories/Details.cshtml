@model TechReviewPoint.Models.Category

@{
    ViewBag.Title = "Details";

    if (Session["AdminSessionEmail"] == null)
    {
        Response.Redirect("~/Admins/AdminLogin");
    }
}

<h2 style="text-align:center">Details</h2>

<div class="container body-content" style="text-align:center">
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.CategoryName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CategoryName)
        </dd>

    </dl>
</div>

<p style="text-align:center">
    @Html.ActionLink("Back To List", "Index", null,
    new { @class = "btn btn-warning" })
</p>
